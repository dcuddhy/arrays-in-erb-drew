<h2>States</h2>


  <ul>
    <% states = ["New York", "Connecticut", "New Jersey"] %>
    <% states.each do |state| %>
    <li><%= state %></li>
    <% end %>
</ul>

<h2>Cities</h2>


  <ul>
    <% cities = ["New York City", "Stamford", "Jersey City", "Hartford", "Trenton", "Albany"] %>
    <% cities.each do |city| %>
    <li><%= city %></li>
    <% end %>
  </ul>
